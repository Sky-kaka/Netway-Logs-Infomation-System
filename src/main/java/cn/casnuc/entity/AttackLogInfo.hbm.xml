<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.casnuc.entity">

	<class name="AttackLogInfo" table="攻击日志表">
		<id name="id" column="事件编号">
			<generator class="native"></generator>
		</id>
		
		<property name="time" column="时间"></property>
		<property name="sourceMac" column="攻击源MAC地址"></property>
		<property name="attackTarget" column="攻击对象"></property>
		<property name="attackType" column="攻击类型"></property>
		<property name="targetIp" column="目标IP地址"></property>
		<property name="targetPort" column="目标端口号"></property>
		<property name="remarks" column="备注"></property>
		
	</class>

</hibernate-mapping>



