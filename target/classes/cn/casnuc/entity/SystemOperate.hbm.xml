<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.casnuc.entity">

	<class name="SystemOperate" table="t_operate_log">
		
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		
		<property name="account" column="account"></property>
		<property name="name" column="name"></property>
		<property name="role" column="role"></property>
		<property name="event" column="event"></property>
		<property name="remarks" column="remarks"></property>
		<property name="time" column="time"></property>
		<property name="ip" column="ip"></property>
		
	</class>

</hibernate-mapping>



