<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.casnuc.entity">

	<class name="Permission" table="t_permission">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		
		<property name="protectLog" column="protectLog"></property>
		<property name="attackLog" column="attackLog"></property>
		<property name="operateLog" column="operateLog"></property>
		<property name="transferConfig" column="transferConfig"></property>
		<property name="protectConfig" column="protectConfig"></property>
		<property name="keywordConfig" column="keywordConfig"></property>
		<property name="gatewayConfig" column="gatewayConfig"></property>
	</class>

</hibernate-mapping>



