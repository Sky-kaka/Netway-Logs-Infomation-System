<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.casnuc.entity">

	<class name="ProtectLogInfo" table="业务防护日志表">
		<id name="id" column="事件编号">
			<generator class="native"></generator>
		</id>
		
		<property name="time" column="时间"></property>
		<property name="fileName" column="文件名称"></property>
		<property name="transferDrection" column="传输方向"></property>
		<property name="serviceType" column="服务类型"></property>
		<property name="sourceIp" column="源IP地址"></property>
		<property name="destinationIp" column="目的IP地址"></property>
		<property name="sourceMac" column="源MAC地址"></property>
		<property name="destinationMac" column="目的MAC地址"></property>
		<property name="sourcePort" column="源端口号"></property>
		<property name="destinationPort" column="目的端口号"></property>
		<property name="transferResult" column="传输结果"></property>
		<property name="fileTypeCheck" column="文件类型检查"></property>
		<property name="fileFormCheck" column="文件格式检查"></property>
		<property name="keyWordCheck" column="关键词检查"></property>
		<property name="remarks" column="备注"></property>
		
	</class>

</hibernate-mapping>



